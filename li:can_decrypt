# is_isomorphic()
def can_decrypt(str1, str2):
  if not str1 or not str2:
    return False

  if len(str1) != len(str2):
    return False

  map1 = {}
  map2 = {}

  for i, c1 in enumerate(str1):
    if c1 in map1 and str2[i] != map1[c1]:
      return False
    elif str2[i] in map2:
      return False
    else:
      map1[c1] = str2[i]
      map2[str1[i]] = c1

  return True
        
       
   
